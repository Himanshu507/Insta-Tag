<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tagfeature">

    <application
        android:name=".InstaTagApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".activities.DashBoardActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activities.InstaActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.TagPhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.SomeOneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.TaggedPhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activities.DragTestActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>